      </Label>
      <Separator layoutX="3.0" layoutY="70.0" prefHeight="4.0" prefWidth="463.0" />
      <JFXPasswordField fx:id="pwdPassword" focusColor="#0400ff" focusTraversable="false" labelFloat="true" layoutX="127.0" layoutY="243.0" prefHeight="37.0" prefWidth="221.0" promptText="password" unFocusColor="#682727">
         <font>
            <Font name="Arial Italic" size="20.0" />
         </font>
      </JFXPasswordField>
      <JFXTextField fx:id="txtUserName" focusColor="#0400ff" focusTraversable="false" labelFloat="true" layoutX="124.0" layoutY="150.0" prefHeight="37.0" prefWidth="221.0" promptText="username" unFocusColor="#682727">
         <font>
            <Font name="Arial Italic" size="20.0" />
         </font>
      </JFXTextField>
      <JFXButton buttonType="RAISED" layoutX="182.0" layoutY="329.0" onAction="#btnLogInOnAction" text="Log In">
