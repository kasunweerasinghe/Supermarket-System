<AnchorPane fx:id="MainLoginFormContext" prefHeight="400.0" prefWidth="469.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainFormController">
   <children>
      <Label layoutX="122.0" layoutY="21.0" text="STOP &amp; SHOP" textFill="#e40707">
         <font>
            <Font name="Arial Black" size="30.0" />
